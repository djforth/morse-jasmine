{
  "name": "@djforth/morse-jasmine-wp",
  "version": "2.2.1",
  "description": "Utilies for Jasmine Tests",
  "main": "index.js",
  "eslintConfig": {
    "extends": "morsedigital",
    "env": {
      "jasmine": true
    },
    "globals": {
      "fdescribe": true,
      "describe": false,
      "beforeEach": false,
      "afterEach": false,
      "it": true,
      "expect": true,
      "spyOn": true
    },
    "rules": {
      "react/jsx-uses-vars": 1,
      "require-jsdoc": 0
    }
  },
  "scripts": {
    "build": "babel src -x '.js'  -d ./",
    "build:watch": "babel src -x '.js' -w --source-maps  -d ./",
    "lint": "eslint src/**"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-jasmine.git"
  },
  "author": "A Stainforh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-jasmine/issues"
  },
  "homepage": "https://github.com/djforth/morse-jasmine#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "benchmark": "^2.1.0",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-config-morsedigital": "^1.0.4",
    "eslint-plugin-jasmine": "^1.8.1",
    "jasmine-core": "^2.4.1",
    "lodash": "^4.13.1",
    "phantomjs-prebuilt": "^2.1.7"
  },
  "dependencies": {},
  "peerDependencies": {
    "jasmine-core": "^2.*.*",
    "lodash": "^4.*.*"
  }
}
